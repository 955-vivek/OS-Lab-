#include <stdio.h>
#include <stdlib.h>
#include <unistd.h>

int main() {
    pid_t pid = fork();

    if (pid > 0) {
        printf("Parent is exiting...\n");
        exit(0);
    } else if (pid == 0) {
        sleep(10);
        printf("Child is sleeping...\n");
    } else {
        printf("No process created\n");
    }
    return 0;
}
